   <head>
      <title>Mad libs</title>
   </head>

   <body>
      <script>
         var username = prompt("Hello ", "");
         var favColor = prompt("Your favourite color is ", "");
         var favSerial = prompt("Your favourite serial is ", "");
         var mobileNo = prompt("Your mobile no is ", "");
         var city = prompt("Your city name is ", "");

         story = username + favColor + favSerial + mobileNo + city
         document.write(story);
      </script>
   </body>

